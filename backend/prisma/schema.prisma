generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

/// This table contains check constraints and requires additional setup for migrations. Visit https://pris.ly/d/check-constraints for more info.
model spatial_ref_sys {
  srid      Int     @id
  auth_name String? @db.VarChar(256)
  auth_srid Int?
  srtext    String? @db.VarChar(2048)
  proj4text String? @db.VarChar(2048)
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model turn_restrictions {
  from_way     Int
  via_node     Int
  to_way       Int
  vehicle_type String?

  @@id([from_way, via_node, to_way])
  @@ignore
}

/// The underlying table does not contain a valid unique identifier and can therefore currently not be handled by Prisma Client.
model routing_nodes {
  id   Int                      @id
  lat  Float
  lon  Float
  geom Unsupported("geometry")?

  @@ignore
}

model routing_edges_bicycle {
  id             Int                      @id
  source         Int
  target         Int
  length_m       Float?
  cost           Float?
  oneway         Boolean?
  oneway_bicycle Boolean?
  maxspeed       Int?
  lanes          Int?
  name           String?
  highway        String?
  surface        String?
  geom           Unsupported("geometry")?
  bicycle        Boolean?                 @default(true)
  reverse_cost   Float?
}

model routing_edges_car {
  id             Int                      @id
  source         Int
  target         Int
  length_m       Float?
  cost           Float?
  oneway         Boolean?
  oneway_bicycle Boolean?
  maxspeed       Int?
  lanes          Int?
  name           String?
  highway        String?
  surface        String?
  geom           Unsupported("geometry")?
  car            Boolean?                 @default(true)
  reverse_cost   Float?
}

model routing_edges_foot {
  id             Int                      @id
  source         Int
  target         Int
  length_m       Float?
  cost           Float?
  oneway         Boolean?
  oneway_bicycle Boolean?
  maxspeed       Int?
  lanes          Int?
  name           String?
  highway        String?
  surface        String?
  geom           Unsupported("geometry")?
  foot           Boolean?                 @default(true)
  reverse_cost   Float?
}
